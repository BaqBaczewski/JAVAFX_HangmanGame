<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EndPageLostController">
   <children>
      <ImageView fx:id="dead" disable="true" fitHeight="150.0" fitWidth="200.0" focusTraversable="true" layoutX="210.0" layoutY="112.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../dead.gif" />
         </image>
      </ImageView>
      <Button fx:id="playAgain" layoutX="233.0" layoutY="305.0" mnemonicParsing="false" onAction="#playAgain" prefHeight="26.0" prefWidth="154.0" text="PLAY AGAIN">
         <font>
            <Font name="Candara Bold Italic" size="12.0" />
         </font></Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="182.0" layoutY="65.0" prefHeight="18.0" prefWidth="245.0" text="GAME OVER" textFill="#ea1212">
         <font>
            <Font name="Candara Bold" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
